{
    "baseUrl": "http://localhost/xdam-v2/xdam/backend/public",
    "api": "/api/v1",
    "options": {
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Accept": "application/json"
        },
        "params": {
            "page": 1,
            "search": "",
            "limit": 20
        }
    },
    "endpoints": {
        "user": {
            "get": {
                "uri": "/user/me",
                "method": "GET"
            }
        },
        "catalogue": {
            "delete": {
                "uri": "/catalogue/clearAll",
                "method": "DELETE"
            },
            "get": {
                "uri": "/catalogue/%(id)s",
                "method": "GET"
            },
            "index": {
                "uri": "/catalogue/%1$s",
                "method": "GET",
                "params": {
                    "page": "1",
                    "limit": "20",
                    "search": ""
                }
            }
        },
        "category": {},
        "resource": {
            "getAll": {
                "uri": "/resource",
                "method": "GET"
            },
            "store": {
                "uri": "/resource",
                "method": "POST"
            },
            "update": {
                "uri": "/resource/%(id)s/update",
                "method": "POST"
            },
            "get": {
                "uri": "/resource/%(id)s",
                "method": "GET"
            },
            "delete": {
                "uri": "/resource/%(id)s",
                "method": "DELETE"
            },
            "deleteFile":{
                "uri": "/resource/%(id)s/deleteAssociatedFiles",
                "method": "PUT"
            },
            "addFile":{
                "uri": "/resource/%(id)s/addFile",
                "method": "POST"
            }
        },
        "tag": {}
    }
}